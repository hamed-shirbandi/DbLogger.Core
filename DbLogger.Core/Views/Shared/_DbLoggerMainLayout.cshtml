

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/css/bootstrap.min.css" />

</head>
<body>
    <nav class="navbar navbar-inverse">
        <div class="container">
            <a class="navbar-brand" href="#">@ViewBag.Title</a>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>ASP.NET Core DB Logger by <a href="http://codeblock.ir/" target="_blank">Hamed Shirbandi</a> </p>
            <p>See project on <a href="https://github.com/hamed-shirbandi/DbLogger.Core" target="_blank"> github</a> </p>
        </footer>
    </div>

</body>
</html>
